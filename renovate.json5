{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "extends": [
    "local>adobe/spacecat-api-service"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "semantic-release"
      ],
      "allowedVersions": ">=23.0"
    },
    {
      "matchPackageNames": [
        "eslint"
      ],
      "allowedVersions": "<9.0.0"
    },
    {
      "matchPackageNames": [
        "zod"
      ],
      "allowedVersions": "<4.0.0"
    }
  ]
}
